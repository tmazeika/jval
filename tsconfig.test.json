{
  "compilerOptions": {
    "module": "es6",
    "lib": ["esnext"],
    "target": "es5",
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "noImplicitOverride": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "esModuleInterop": true
  },
  "include": ["tests"]
}
