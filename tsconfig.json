{
  "compilerOptions": {
    "module": "esnext",
    "lib": ["esnext"],
    "target": "es5",
    "moduleResolution": "node",
    "outDir": "dist",
    "declaration": true,
    "emitDeclarationOnly": true,
    "strict": true,
    "noImplicitOverride": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "esModuleInterop": true
  },
  "include": ["src"]
}
