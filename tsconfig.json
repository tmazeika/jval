{
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["esnext"],
    "target": "es6",
    "declaration": true,
    "emitDeclarationOnly": true,
    "outFile": "dist/index.d.ts",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "esModuleInterop": true
  },
  "include": ["src"]
}
